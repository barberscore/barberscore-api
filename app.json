{
  "name": "Barberscore Django",
  "addons": [
    "cloudinary:starter",
    "docraptor:starter",
    "geocody:test",
    "heroku-postgresql:standard-0",
    "scheduler:standard",
    "sendgrid:starter",
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "description": "Scoring software for the Barbershop Harmony Society",
  "environments": {
    "test": {
      "scripts": {
        "test": "pytest"
      }
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "standard-2x"
    }
  },
  "logo": "https://s3-us-west-1.amazonaws.com/barberscore/static/images/bhs_logo.png",
  "repository": "https://github.com/dbinetti/barberscore-django",
  "stack": "cedar-14",
  "scripts": {
    "postdeploy": "django-admin migrate --noinput"
  }
}
